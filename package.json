  "main": "app.js",
  "scripts": {
    "build": "cd client && webpack --mode production",
    "local": "npm run local-build && npm run local-env && node -r dotenv/config app",
    "local-build": "cd client && webpack --mode=development",
    "local-env": "heroku config | sed '1,1 d' | sed s/': *'/=/ > .env && echo NODE_ENV=development >> .env",
    "start": "node app",
    "heroku-postbuild": "npm run build"
  },
