  "main": "app.js",
  "scripts": {
    "heroku-postbuild": "cd client && webpack --config webpack.prod.js",
    "lint": "eslint --ignore-path .gitignore ./",
    "local": "npm-run-all -p local-hot-backend local-hot-frontend",
    "local-build": "cd client && webpack --config webpack.dev.js",
    "local-env": "cd scripts && bash local-env.sh",
    "local-hot-backend": "nodemon -r dotenv/config app --ignore client",
    "local-hot-frontend": "cd client && webpack-dev-server --config webpack.dev.js",
    "start": "cross-env NODE_ENV=production node app"
  },
